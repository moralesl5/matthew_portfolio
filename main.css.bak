/* OVERLAY */
.overlay{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	height: 100%;
	width: 100%;
	opacity: 1;
	transition: 3s ease;
	background-color: rgba(0,0,0,0.7); 
	display: block;
}

.hidden{
	display: none;
}

.visuallyHidden{
	opacity: 0;
}

