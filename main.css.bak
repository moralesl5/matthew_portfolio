/*Fake-Trigger*/
#sized {
    color:transparent;
    background-color: transparent;
    border: transparent;
}

/*Modal-Box*/
.modal {
    display: none;
    position: fixed;
    z-index: 1;
    /*padding:0.5%;*/
    left: 0;
    top: 0;
    width: 100%; 
    height: 100%;
    overflow: hidden;
    opacity: 0.85;
}

.modal-content {
    position: relative;
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2),0 6px 20px 0 rgba(0,0,0,0.19);
    -webkit-animation-name: animatetop;
    -webkit-animation-duration: 0.4s;
    animation-name: animatetop;
    animation-duration: 0.4s;
    background-color: white;
}

.modal-header {
    padding: 2px 16px;
    background-color: white;
    border-bottom: none;
    height:35vh;
}

.modal-body {
    padding: 2px 16px;
    font-family: matthewFont;
    text-rendering: geometricPrecision;
    font-weight: 100;
}

.modal-footer {
    padding: 2px 16px;
    color: black;
    border-top: none;
    text-align: center;
}

.modal-footer hr {
    margin-top: 20vh;
}

.modal-footer>p {
  color: #468966
}

.modal-footer h3 {
    text-transform: none;
    font-size:4vh;

}

.close {
    background-image: url(./resources/X.png);
    display: block;
	position: absolute;
	right: 25px;
	top: 54px;
	width: 70px;
	height: 70px;
	background-repeat: no-repeat;
	background-size: 35px 30px;
	border: none;
	outline: none;
}

.close:hover,
.close:focus {
    text-decoration: none;
    cursor: pointer;
}

@media only screen and (max-width: 980px) {
	.close{
		right: 15px;
		top: 35px;
		width: 50px;
		height: 50px;
		background-size: 25px 20px;
	}

}

@-webkit-keyframes zoomIn {
    from {
        opacity: 0;
        -webkit-transform: scale3d(.3, .3, .3);
        transform: scale3d(.3, .3, .3);
    }

    50% {
        opacity: 1;
    }
}

.zoomIn {
    -webkit-animation-name: zoomIn;
    animation-name: zoomIn;
}

.animated {
    -webkit-animation-duration: .3s;
    animation-duration: .3s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
}


/*Navigation Inner Styling*/

.nav ul li a:hover{
    text-decoration: none;
    color: #B64926;
}

.nav ul li a{
    padding:1em;
    text-transform: uppercase;
    font-weight: 700;
    font-size:3vw;
    letter-spacing: -0.05em;
}

.nav ul li{
    display: inline-block;
    font-size: 3em;
    cursor: pointer;
}

.nav ul{
    clear: both;
    width: 100%;
    text-align: center;
}

#comingSoon{
   	left: 50%;
    margin-right: -50%;
	position: absolute;
	font-size: 12px;
}

#comingSoon span{
	text-align: center;
}

#navContact{
	margin-top: 70px;
}



/*Mobile Optimization*/

/* Landscape-Phone to Portrait Tablet*/
@media (max-width:767px) {
  
    .nav ul li{
        display: block;
        height:38px;
    }

    .nav ul {
        clear: both;
        width: 100%;
        text-align: center;
        padding: 0;
    }

    .nav ul li a {
        padding: 0;
        text-transform: uppercase;
        font-weight: 700;
        font-size: 6vw;
        letter-spacing: -0.05em;
    }
}
  
/*Landscape-Phone Downwards*/
@media (max-width: 480px) {

	#comingSoon{
		left: 40%;
    	margin-right: -50%;
		position: absolute;
		font-size: 12px;
	}
}



